{
	"$schema": "http://json-schema.org/schema",
	"cli": "nx",
	"$id": "sdk",
	"type": "object",
	"properties": {
		"name": {
			"type": "string",
			"description": "Module Name",
			"visible": true,
			"x-prompt": "What name would you like to use for this sdk?",
			"$default": {
				"$source": "argv",
				"index": 0
			}
		}
	},
	"required": ["name"]
}
